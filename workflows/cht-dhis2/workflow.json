{
  "workflow": {
    "steps": [
      {
        "id": "map-submission",
        "adaptors": [
          "collections, common@2.3.1"
        ],
        "expression": "map-cht-submission.js",
        "next": {
          "check-events": "!state.errors"
        }
      },
      {
        "id": "check-events",
        "adaptor": "dhis2@6.3.1",
        "configuration": "tmp/dhis2-creds.json",
        "expression": "check-events.js",
        "next": {
          "create-event": "!state.errors"
        }
      },
      {
        "id": "create-event",
        "adaptor": "dhis2@6.3.1",
        "configuration": "tmp/dhis2-creds.json",
        "expression": "create-event.js"
      }
    ]
  }
}